<xml version="1.0">
<xds:schema xmlns="http://www.w3.org/2001/XMLSchema">

 <xds:element name="information">
   <xds:complexType>
      <xds:sequence>
         <xds:element ref="Name" minOccurs="1" maxOccurs="1" type="xds:string" />
		 <xds:element ref="utcid" minOccurs="1" maxOccurs="1" type="xds:string" />
		 <xds:element ref="address" minOccurs="1" maxOccurs="2" type="xds:string">
		   <xds:attribute name="line" type="xds:integer" use="required" />
		 <xds:element>
		 <xds:element ref="phone" type="xds:integer" />
		 <xds:element ref="degreetype >
		   <xds:attribute name="type" use="required" type="xds:string" />
		 </xds:element>
		 <xds:element ref="major" minOccurs="1" maxOccurs="1" type="xds:string">
		 <xds:element ref="concentration" minOccurs="1" maxOccurs="1" type="xds:string">
		 <xds:element ref="graduationdate" minOccurs="1" maxOccurs="1" type="xds:date">
		 <xds:element ref="utcemail" minOccurs="1" maxOccurs="1" type="xds:string">
      </xds:sequence>
   </xds:complexType>
 </xds:element>

</xds:schema>
